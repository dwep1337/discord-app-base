{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "Bot Discord usando TypeScript e POO",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "watch": "tsc --watch",
    "register": "ts-node src/scripts/register.ts",
    "delete-commands": "ts-node src/scripts/deleteCommands.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "pidusage": "^4.0.1",
    "systeminformation": "^5.27.11"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
